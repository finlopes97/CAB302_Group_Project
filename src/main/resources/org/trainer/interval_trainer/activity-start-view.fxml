<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.trainer.interval_trainer.Activity.ActivityController"
            prefHeight="400.0" prefWidth="600.0">



<VBox alignment="CENTER">
    <Label fx:id="timerLabel" />
    <Button text="Start" onAction="#timer"/>
<Button text="Back" onAction="#onArrowButtonClick"/>
<Button text="Pause" onAction="#onPauseButtonClick"/>

</VBox>
</AnchorPane>