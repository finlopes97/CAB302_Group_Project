<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.trainer.interval_trainer.controller.activity.ActivityController"
            prefHeight="400.0" prefWidth="600.0">


<VBox alignment="CENTER" spacing="10.0">
    <padding>
        <Insets bottom="190" left="190" right="190" top="190"/>
    </padding>
    <Label fx:id="name" text="name"/>
    <Label fx:id="debug" />
    <Label fx:id="timerLabel"/>
    <Button text="START" onAction="#onStart" />
    <Button text="Back" onAction="#onArrowButtonClick"/>
    <Button text="Next" onAction="#onNext"/>
    <Button text="Prev" onAction="#onPrev"/>
    <Button text="pause" onAction="#onPauseButtonClick"/>
    <VBox fx:id="children"/>
</VBox>

</AnchorPane>

