<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>


<VBox id="group" prefHeight="154.0" prefWidth="588.0" xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1">
    <HBox alignment="CENTER_LEFT" prefHeight="24.0" prefWidth="564.0" spacing="10.0">
        <children>
            <TextField minWidth="-Infinity" prefHeight="26.0" prefWidth="267.0" promptText="Name"/>
            <Region prefHeight="25.0" prefWidth="345.0" HBox.hgrow="ALWAYS"/>
            <Label alignment="CENTER" text="X"/>
            <TextField minWidth="-Infinity" prefHeight="26.0" prefWidth="60.0" promptText="Reps"/>
            <Button fx:id="delete" onAction="#deleteGroup" minWidth="-Infinity" prefHeight="26.0" prefWidth="89.0" text="delete"/>
        </children>
    </HBox>
    <HBox alignment="TOP_CENTER" prefHeight="89.0" prefWidth="564.0">
        <Button fx:id="addchild" onAction="#addChildGroup" text="Add Group">
            <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
        </Button>
        <Button fx:id="addItem" onAction="#addChildBlock" text="add item">
            <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
        </Button>
    </HBox>
    <VBox id="childrens" fx:id="children" prefHeight="90.0" prefWidth="564.0" spacing="3.0">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <VBox.margin>
            <Insets/>
        </VBox.margin>
    </VBox>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>

</VBox>