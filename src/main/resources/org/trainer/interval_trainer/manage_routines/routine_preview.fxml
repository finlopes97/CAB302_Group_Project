<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="88.0" prefWidth="383.0" style="-fx-background-color: gray; -fx-background-radius: 10; -fx-border-color: black; -fx-border-radius: 10;" type="VBox" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="name" text="Label">
         <font>
            <Font name="System Bold" size="16.0" />
         </font></Label>
      <Label fx:id="description" text="Label" />
      <ButtonBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="42.0" prefWidth="400.0">
        <buttons>
            <Button fx:id="delete" mnemonicParsing="false" onAction="#delete" text="delete" />
            <Button fx:id="edit" mnemonicParsing="false" onAction="#edit" text="edit" />
            <Button mnemonicParsing="false" onAction="#play" text="Play" />
        </buttons>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </ButtonBar>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
